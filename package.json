{
  "name": "vue-compare-image",
  "version": "0.2.0",
  "description": "Vue component to compare two images using slider.",
  "author": "Shota Tamura",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:lib": "vue-cli-service build --target lib --name vueCompareImage ./src/VueCompareImage.vue",
    "prepublishOnly": "npm run build:lib"
  },
  "main": "dist/vueCompareImage.common.js",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "dependencies": {
    "core-js": "^3.47.0",
    "css-element-queries": "^1.2.3",
    "vue": "^2.7.16"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^5.0.9",
    "@vue/cli-plugin-eslint": "^5.0.9",
    "@vue/cli-service": "^5.0.9",
    "@babel/eslint-parser": "^7.25.1",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^9.33.0",
    "sass": "^1.83.0",
    "sass-loader": "^16.0.6",
    "vue-template-compiler": "^2.7.16"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-mixed-operators": 0,
      "no-irregular-whitespace": 0,
      "prefer-template": 0
    },
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "keywords": [
    "picture comparison",
    "image comparison",
    "slider",
    "react",
    "twentytwenty"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "junkboy0315/vue-compare-image"
  },
  "volta": {
    "node": "24.11.0"
  }
}
