{
  "name": "vue-compare-image",
  "version": "0.1.1",
  "description": "Vue component to compare two images using slider.",
  "author": "Shota Tamura",
  "license": "MIT",
  "keywords": [
    "picture comparison",
    "image comparison",
    "slider",
    "react",
    "twentytwenty"
  ],
  "main": "dist/vueCompareImage.common.js",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:lib": "vue-cli-service build --target lib --name vueCompareImage ./src/VueCompareImage.vue",
    "lint": "vue-cli-service lint",
    "prepublishOnly": "npm run build:lib"
  },
  "dependencies": {
    "css-element-queries": "^1.0.2",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.3",
    "@vue/cli-plugin-eslint": "^3.0.3",
    "@vue/cli-service": "^3.0.3",
    "@vue/eslint-config-airbnb": "^3.0.4",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "rules": {
      "no-mixed-operators": 0,
      "no-irregular-whitespace": 0,
      "prefer-template": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
